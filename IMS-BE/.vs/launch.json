{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "IMS-BE (python -m src.app)",
      "type": "python",
      "request": "launch",
      "module": "src.app",
      "cwd": "${workspaceFolder}",
      "env": {
        "DATABASE_URL": "sqlite:///ims.db",
        "INIT_DB": "1",
        "SEED_ON_START": "0",
        "PORT": "5000"
      },
      "console": "integratedTerminal",
      "justMyCode": true
    }
  ]
}
